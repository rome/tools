{
  "name": "@romejs-editor/vscode",
  "main": "./lib/main",
  "version": "0.6.2",
  "description": "Rome LSP Atom extension",
  "license": "MIT",
  "engines": {
    "atom": ">=1.21.0"
  },
  "dependencies": {
