{
	"name": "@romejs-integration/vscode",
	"displayName": "Rome",
	"description": "Rome LSP VSCode extension",
	"version": "0.0.1",
	"activationEvents": [
		"*"
	],
	"icon": "icon.png",
	"_activationEvents": [
		"onLanguage:javascript",
		"onLanguage:javascriptreact",
		"onLanguage:typescript",
		"onLanguage:typescriptreact",
		"onLanguage:json"
	],
	"files": [
		"/icon.png",
		"/README.md"
	],
	"main": "index.ts",
	"engines": {
		"vscode": "^1.33.0"
	},
	"dependencies": {
		"vscode-languageclient": "^5.2.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/romejs/rome.git"
	},
	"bugs": {
		"url": "https://github.com/romejs/rome/issues"
	},
	"rome": {
		"extends": "../../../package.json",
		"bundler": {
			"externals": [
				"vscode"
			]
		}
	}
}
