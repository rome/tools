            </svg>
            <h1 class="logo-text">Rome</h1>
          </a>
        </div>
        <nav aria-labelledby="site-navigation" class="menu">
          <h2 id="site-navigation" class="sr-only">Site Navigation</h2>
          <ul>
            <li>
              <a href="/rules">Rules</a>
            </li>
          </ul>
        </nav>
        <nav aria-labelledby="page-navigation" class="table-of-contents">
          <h2 id="page-navigation" class="sr-only">Page Navigation</h2>
          <div class="toc-container">
            {{ content | toc(tags=['h1', 'h2'], wrapper='div  ', wrapperClass='fixed toc')  | safe }}
          </div>
        </nav>
        <div class="mode-switch-container">
          <button id="mode-switch" class="mode-switch">
            Dark Mode
          </button>
        </div>
      </div>
      <div class="main">
        <div class="overlay"></div>

        {% if showHero %}
        <div class="hero">
          <h1>Your next-gen JavaScript toolchain.</h1>
          <h2>Focus on code, let Rome take care of the rest!</h2>
        </div>
        {% endif %}

        <main id="main-content" class="content">

          {{ content | safe }}

        </main>
      </div>
    </div>

    <footer aria-labelledby="footer" class="footer">
      <h1 id="footer" class="sr-only">Footer</h1>

      <div class="container">
      <div class="row">
        <div>
          <nav aria-labelledby="community-navigation">
            <h2 id="community-navigation">Community</h2>
            <ul>
              <li>
                <a href="https://github.com/romejs/rome/blob/master/.github/CODE_OF_CONDUCT.md">Code of Conduct</a>
              </li>
              <li>
                <a href="https://discord.gg/9WxHa5d">Discord</a>
              </li>
            </ul>
          </nav>
        </div>
        <div>
          <nav aria-labelledby="resources-navigation">
            <h2 id="resources-navigation">Resources</h2>
            <ul>
              <li>
                <a href="https://github.com/romejs/rome">GitHub</a>
              </li>
            </ul>
          </nav>
        </div>
      </div>
      <div class="logo">
        <svg data-name="logo"
          xmlns="http://www.w3.org/2000/svg" viewBox="0 0 360 360">
          <defs>
            <style>.cls-1{fill:#27272a;}.cls-2{fill:#ffc905;}</style>
          </defs>
