{
  "private": true,
  "version": "0.0.2",
  "//": "Look! No deps!",
  "dependencies": {},
  "///": "Only used for static type checking",
  "devDependencies": {
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "typescript": "^3.7.0"
  },
  "prettier": {
    "version": "1.19.1",
    "trailingComma": "all",
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSpacing": false,
    "jsxBracketSameLine": true
  },
  "rome": {
    "isolated": true,
    "bundler": {
      "mode": "modern"
    },
    "lint": {
      "ignore": [
        "build",
        "coverage",
        "flow-typed",
        "node_modules",
        "core/static",
        "resources",
        "__generated__",
        "__fixtures__",
        "vendor",
        "type-libs",
        "lib/modules.js"
      ]
    }
  }
}
